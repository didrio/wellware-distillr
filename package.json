{
  "name": "wellware-distillr",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "build-web": "npx expo export:web",
    "predeploy": "expo export:web",
    "deploy-hosting": "npm run predeploy && firebase deploy --only hosting",
    "build-dev-android": "eas build --profile development --platform android",
    "build-dev-ios": "eas build --profile development --platform ios",
    "build-dev": "npm run build-dev-android && npm run build-dev-ios",
    "build-prod-android": "eas build --profile production --platform android",
    "build-prod-ios": "eas build --profile production --platform ios",
    "build-prod": "npm run build-prod-android && npm run build-prod-ios",
    "start-dev": "npx expo start --dev-client"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "expo": "~48.0.15",
    "expo-dev-client": "~2.2.1",
    "expo-status-bar": "~1.4.4",
    "firebase": "^9.21.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.7",
    "react-native-get-random-values": "~1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-purchases": "^5.14.0",
    "react-native-web": "~0.18.10",
    "react-responsive": "^9.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/webpack-config": "^18.0.4",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0"
  },
  "private": true
}
